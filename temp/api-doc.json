    "/api/pipelines/job/{jobId}": {
      "get": {
        "tags": [
          "pipeline-controller"
        ],
        "summary": "Job 상세 조회 API",
        "description": "특정 Job에 대한 상세 정보를 조회하는 API 입니다.",
        "operationId": "getJobInfo",
        "parameters": [
          {
            "name": "jobId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
