    },
    "/api/pipelines/{jobId}/jobs/logs": {
      "get": {
        "tags": [
          "pipeline-controller"
        ],
        "summary": "Job 로그 조회 API",
        "description": "특정 Job에 실행 로그를 반환하는 API 입니다.",
        "operationId": "getJobsLogs",
        "parameters": [
          {
            "name": "jobId",
