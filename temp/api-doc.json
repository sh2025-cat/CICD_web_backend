          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "실행 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/CommonResponse"
                }
              }
            }
          },
          "404": {
            "description": "존재하지 않는 Job ID",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/deployments/ecs": {
      "post": {
        "tags": [
          "deployment-controller"
        ],
        "operationId": "deployToEcs",
