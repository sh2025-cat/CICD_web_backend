{
  "openapi": "3.1.0",
  "info": {
    "title": "CI/CD Web App API",
    "description": "CI/CD Webb App API 명세서 및 테스트",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8080",
      "description": "Generated server url"
    }
  ],
  "paths": {
    "/api/webhook/github": {
      "post": {
        "tags": [
          "webhook-controller"
        ],
        "operationId": "handleGithubWebhook",
        "parameters": [
          {
            "name": "X-GitHub-Event",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "additionalProperties": {

                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/pipelines/{workflowId}/start": {
      "post": {
        "tags": [
          "pipeline-controller"
        ],
        "summary": "특정 Pipeline 실행",
        "description": "특정 workflow 실행 명령 API 입니다.",
        "operationId": "runJob",
        "parameters": [
          {
            "name": "workflowId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          },
          {
            "name": "branch",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "default": "main"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GitHubBaseRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "실행 성공"
          },
          "404": {
            "description": "존재하지 않는 Job ID",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/pipelines/{runId}/rerun": {
      "post": {
        "tags": [
          "pipeline-controller"
        ],
        "summary": "특정 Pipeline 재실행",
        "description": "특정 workflow 재실행 명령 API 입니다.",
        "operationId": "rerunJob",
        "parameters": [
          {
            "name": "runId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GitHubBaseRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "실행 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/CommonResponse"
                }
              }
            }
          },
          "404": {
            "description": "존재하지 않는 Job ID",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/pipelines/{runId}/cancel": {
      "post": {
        "tags": [
          "pipeline-controller"
        ],
        "summary": "특정 Pipeline 정지",
        "description": "특정 workflow 정지 명령 API 입니다.",
        "operationId": "cancelJob",
        "parameters": [
          {
            "name": "runId",
            "in": "path",
            "required": true,
